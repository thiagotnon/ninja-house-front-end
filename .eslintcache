nstrução de Frontend/trabalho_final/ninja_house/src/pages/Guests/Guests.jsx",["225"],"import React from \"react\";\r\nimport { Alert, Button, ButtonGroup, Card, Col, Row } from \"react-bootstrap\";\r\nimport GuestService from \"../../services/GuestService\";\r\nimport \"../../styles/units.css\";\r\nimport { Link } from \"react-router-dom\";\r\nimport Page from \"../../components/Page\";\r\nimport { FaRegEdit, FaUserPlus, FaUserTimes } from \"react-icons/fa\";\r\nimport { formatCpf, padLeadingZeros } from \"../../helpers/functions\";\r\nimport swal from \"sweetalert\";\r\nimport UnitsService from \"../../services/UnitsService\";\r\n\r\nconst Guests = (props) => {\r\n  const [guests, setGuests] = React.useState([]);\r\n  const [unit, setUnit] = React.useState([]);\r\n\r\n  React.useEffect(() => {\r\n    GuestService.getAll().then((results) => {\r\n      setGuests(results.data.data);\r\n    });\r\n  }, []);\r\n\r\n  React.useEffect(() => {\r\n    UnitsService.get(guests.unit_number).then((results) => {\r\n      setUnit(results.data);\r\n    });\r\n  }, [guests]);\r\n  console.log(guests);\r\n  function handleClick(id) {\r\n    swal({\r\n      title: \"Você realmente deseja remover o hóspede?\",\r\n      text: \"Não será possível recupera-la após a remoção.\",\r\n      icon: \"warning\",\r\n      buttons: [\"Cancelar\", true],\r\n      dangerMode: true,\r\n    }).then((willDelete) => {\r\n      if (willDelete) {\r\n        GuestService.delete(id)\r\n          .then(() => {\r\n            GuestService.getAll().then((results) => {\r\n              setGuests(results.data.data);\r\n            });\r\n          })\r\n          .catch((error) => {\r\n            console.log(error.response.data);\r\n          });\r\n        swal(\"Poof! O hóspede foi removido com sucesso!\", {\r\n          icon: \"success\",\r\n        });\r\n      }\r\n    });\r\n\r\n    props.history.push(\"/hospedes\");\r\n  }\r\n  console.log(guests);\r\n  return (\r\n    <>\r\n      <Page\r\n        title=\"Hospedes\"\r\n        custom_content={\r\n          <Link\r\n            to=\"/hospede/novo\"\r\n            className=\"btn btn-outline-secondary d-flex align-items-center justify-content-between\"\r\n          >\r\n            <FaUserPlus size={20} className=\"mr-2\" />\r\n            Adicionar Hospede\r\n          </Link>\r\n        }\r\n      >\r\n        {guests.length === 0 ? (\r\n          <>\r\n            <Alert variant=\"secondary\">\r\n              Não existem hospedes registrados até o momento.\r\n            </Alert>\r\n          </>\r\n        ) : (\r\n          <Row>\r\n            {guests.map((guest) => (\r\n              <Col md={4} key={guest.id} className=\"mb-4\">\r\n                <Card>\r\n                  <Card.Body>\r\n                    <div className=\"d-flex align-items-center justify-content-between\">\r\n                      <h5 className=\"font-weight-bold\">{guest.name}</h5>\r\n                      <p>\r\n                        <small>Apto.</small>\r\n                        <br />{\" \"}\r\n                        <strong>\r\n                          {padLeadingZeros(guest.apartments.unit_number, 3)}\r\n                        </strong>\r\n                      </p>\r\n                      <p>\r\n                        <small>Bloco</small>\r\n                        <br /> <strong>{guest.apartments.block}</strong>\r\n                      </p>\r\n                      <p>\r\n                        <small>Andar</small>\r\n                        <br /> <strong>{guest.apartments.floor}º</strong>\r\n                      </p>\r\n                    </div>\r\n                    <hr />\r\n                    <div className=\"box-contact d-flex align-items-center justify-content-between\">\r\n                      <div className=\"phone d-flex align-items-center\">\r\n                        <p>\r\n                          <strong>RG: </strong> {guest.rg}\r\n                        </p>\r\n                      </div>\r\n                      <div className=\"mail\">\r\n                        <p>\r\n                          {\" \"}\r\n                          <strong>CPF: </strong> {formatCpf(guest.cpf)}\r\n                        </p>\r\n                      </div>\r\n                      <ButtonGroup>\r\n                        <Link\r\n                          className=\"btn btn-outline-secondary\"\r\n                          to={`/hospede/${guest.id}/editar`}\r\n                        >\r\n                          <FaRegEdit size={20} /> Editar\r\n                        </Link>\r\n\r\n                        <Button\r\n                          variant=\"outline-danger\"\r\n                          onClick={() => handleClick(guest.id)}\r\n                        >\r\n                          <FaUserTimes size={20} />\r\n                        </Button>\r\n                      </ButtonGroup>\r\n                    </div>\r\n                  </Card.Body>\r\n                </Card>\r\n              </Col>\r\n            ))}\r\n          </Row>\r\n        )}\r\n      </Page>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Guests;\r\n",["226","227"],"/mnt/f/IESB/Construção de Frontend/trabalho_final/ninja_house/src/pages/Orders/Orders.jsx",[],"/mnt/f/IESB/Construção de Frontend/trabalho_final/ninja_house/src/pages/LeisureSpaces/LeisureSpaceForm.jsx",["228"],"/mnt/f/IESB/Construção de Frontend/trabalho_final/ninja_house/src/pages/Guests/GuestForm.jsx",[],"/mnt/f/IESB/Construção de Frontend/trabalho_final/ninja_house/src/pages/Employees/EmployeeForm.jsx",[],["229","230"],"/mnt/f/IESB/Construção de Frontend/trabalho_final/ninja_house/src/pages/Employees/Employees.jsx",[],"/mnt/f/IESB/Construção de Frontend/trabalho_final/ninja_house/src/pages/Dwellers/DwellerForm.jsx",[],"/mnt/f/IESB/Construção de Frontend/trabalho_final/ninja_house/src/pages/Home.jsx",[],"/mnt/f/IESB/Construção de Frontend/trabalho_final/ninja_house/src/Routes.jsx",[],"/mnt/f/IESB/Construção de Frontend/tra